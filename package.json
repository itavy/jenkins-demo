{
  "name": "jenkins-demo",
  "version": "1.0.0",
  "description": "jenkins demo",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test:check-style": "./node_modules/.bin/eslint index.js \"lib/**/*.js\" \"test/**/*.js\"",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "pretest": "npm run test:check-style"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/itavy/jenkins-demo.git"
  },
  "author": "Octavian Ionescu <itavyg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itavy/jenkins-demo/issues"
  },
  "homepage": "https://github.com/itavy/jenkins-demo#readme",
  "pre-commit": {
    "run": [
      "test:check-style"
    ]
  },
  "nyc": {
    "all": true,
    "color": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "watermarks": {
      "check-coverage": true,
      "statements": [
        50,
        90
      ],
      "lines": [
        50,
        90
      ],
      "functions": [
        50,
        90
      ],
      "branches": [
        50,
        90
      ]
    }
  },
  "devDependencies": {
    "@itavy/test-utilities": "^6.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eyes": "^0.1.8",
    "jaguarjs-jsdoc": "^1.1.0",
    "jsdoc": "^3.6.2",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "tap": "^14.3.1"
  },
  "dependencies": {
    "fastify": "^2.6.0",
    "js-yaml": "^3.13.1",
    "mongodb": "^3.2.7",
    "redis": "^2.8.0",
    "semver": "^6.2.0"
  }
}
